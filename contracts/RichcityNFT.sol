    using SafeMath for uint256;
    // mint price
    uint256 public PUBLIC_PRICE = 20;
    //white mint price
    uint256 public ONE_WHITE_PRICE = 10;
    uint256 public TWO_WHITE_PRICE = 15;

    uint256 public WHITE_SALE_TIME;
    uint256 public PUBLIC_SALE_TIME;

    uint256 public immutable MAX_TOKENS;

    uint256 public rewardAmount;
    uint256 public rewardTime;
    uint256 public lastMintTime;
    uint256 public round;
    mapping(address => uint256) public userReward;
    address[] public mintAccounts;
    // number of tokens have been minted so far
    uint16 public minted;

    struct RichCity {
        bool isGangster;
        uint8 body;
        uint8 level;
        uint8 clothes;
        uint8 hair;
        uint8 jewelry;
        uint8 tatto;
        uint8 belt;
        uint8 sunglass;
        uint8 hat;
        uint8 mask;
        uint8 weapon;
        uint8 gen;
    }

    mapping(uint256 => RichCity) public tokenTraits;
    // used to ensure there are no duplicates
    mapping(uint256 => uint256) existingCombinations;
    //whiteListed
    mapping(address => bool) public whiteListedOne;
    mapping(address => bool) public whiteListedTwo;
    //mintd
    uint256 public SWATMinted;
    uint256 public GangsterMinted;
    uint256 public LEADMinted;
    mapping(address => uint256) public mintCount;

    IERC20 public CASH;
    IERC20 public RICH;
    IERC20 public USDC;

    ISwapV2Factory public factory;
    ISwapV2Router public router;
    address public bank;
    ITraits public traits;

    IRandomSource public randomSource;

    bool private _reentrant = false;

    modifier nonReentrant() {
        require(!_reentrant, "No reentrancy");
        _reentrant = true;
        _;
        _reentrant = false;
    }

    constructor(
        ISwapV2Router _router,
        IERC20 _RICH,
        IERC20 _USDC,
        IERC20 _CASH,
        ITraits _traits,
        IRandomSource _randomSource,
        uint256 _maxTokens,
        uint256 _WHITE_SALE_TIME,
        uint256 _PUBLIC_SALE_TIME
    ) ERC721("Rich City Game", "RCG") {
        RICH = _RICH;
        USDC = _USDC;
        CASH = _CASH;
        traits = _traits;
        randomSource = _randomSource;
        MAX_TOKENS = _maxTokens;
        factory = ISwapV2Factory(_router.factory());
        router = _router;
        WHITE_SALE_TIME = _WHITE_SALE_TIME;
        PUBLIC_SALE_TIME = _PUBLIC_SALE_TIME;
    }

    function mint(uint256 amount, IERC20 token) external nonReentrant {
        require(token == RICH || token == USDC);
        require(tx.origin == _msgSender(), "Only EOA");
        require(minted + amount <= MAX_TOKENS, "All NFTs are minted");
        require(amount > 0 && amount <= 15, "Invalid mint amount");
        require(block.timestamp > WHITE_SALE_TIME, "not start");
        rebase();
        uint256 costValue = PUBLIC_PRICE;
        if (block.timestamp < PUBLIC_SALE_TIME && minted <= 1000) {
            require(minted + amount <= 1000, "Mint exceed");
            require(
                whiteListedOne[_msgSender()] || whiteListedTwo[_msgSender()],
                "Not in Whitelist"
            );
            require(
                mintCount[_msgSender()] + amount <= 10,
                "Each address can mint 10"
            );

            if (whiteListedOne[_msgSender()] && whiteListedTwo[_msgSender()]) {
                costValue = ONE_WHITE_PRICE;
            } else if (whiteListedTwo[_msgSender()]) {
                costValue = TWO_WHITE_PRICE;
            } else {
                costValue = ONE_WHITE_PRICE;
            }
        }

        uint256 totalCost = 0;
        uint256 seed;
        uint256 firstMinted = minted;

        for (uint256 i = 0; i < amount; i++) {
            minted++;
            mintAccounts.push(msg.sender);
            mintCount[_msgSender()]++;
            seed = random(minted);
            randomSource.update(minted ^ seed);
            generate(minted, seed);
            if (block.timestamp < PUBLIC_SALE_TIME && minted <= 1000) {
                totalCost += costValue;
            } else {
                totalCost += mintCost(minted);
            }
            tokenTraits[minted].isGangster ? GangsterMinted++ : SWATMinted++;
            _safeMint(msg.sender, firstMinted + i + 1);
        }

        lastMintTime = block.timestamp;
        if (rewardTime < 43200) rewardTime += 3600;
        rewardAmount += totalCost.mul(10**RICH.decimals()).div(10);

        swapAndAddLiquidity(totalCost, token);
    }

    function rebase() public {
        if (
            block.timestamp >= lastMintTime + rewardTime &&
            rewardAmount > 0 &&
            mintAccounts.length >= 10
        ) {
            uint256 len = mintAccounts.length;
            userReward[mintAccounts[len - 1]] += rewardAmount.div(100).mul(51);
            userReward[mintAccounts[len - 2]] += rewardAmount.div(100).mul(20);
            userReward[mintAccounts[len - 3]] += rewardAmount.div(100).mul(15);
            for (uint256 i = len - 10; i < len - 3; i++) {
                userReward[mintAccounts[i]] += rewardAmount.div(100).mul(2);
            }
            round++;
            rewardTime = 0;
        }
    }

    function claimReward() external {
        require(userReward[msg.sender] > 0);
        RICH.safeTransfer(msg.sender, userReward[msg.sender]);
        rewardAmount -= userReward[msg.sender];
        userReward[msg.sender] = 0;
    }

    function upgrade(uint16[] calldata tokenIds) external {
        for (uint256 id = 0; id < tokenIds.length; id++) {
            require(ownerOf(tokenIds[id]) == msg.sender);
            RichCity storage t = tokenTraits[tokenIds[id]];
            require((t.isGangster && t.level < 5) || t.level < 3);
            if (t.isGangster && t.level == 3) {
                upgradeGenerate(tokenIds[id], random(minted));
                LEADMinted++;
                GangsterMinted--;
            }
            uint256 costValue = upgradeCost(tokenIds[id]);
            CASH.burnFrom(msg.sender, costValue * (10**CASH.decimals()));
            t.level++;
        }
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override nonReentrant {
        // Hardcode the Bank's approval so that users don't have to waste gas approving
        if (_msgSender() != address(bank))
            require(
                _isApprovedOrOwner(_msgSender(), tokenId),
                "ERC721: transfer caller is not owner nor approved"
            );
        _transfer(from, to, tokenId);
    }

    /***INTERNAL */

    /**
     * generates traits for a specific token, checking to make sure it's unique
     * @param tokenId the id of the token to generate traits for
     * @param seed a pseudorandom 256 bit number to derive traits from
     * @return t - a struct of traits for the given token ID
     */
    function generate(uint256 tokenId, uint256 seed)
        internal
        returns (RichCity memory t)
    {
        t = selectTraits(seed);
        if (existingCombinations[structToHash(t)] == 0) {
            tokenTraits[tokenId] = t;
            existingCombinations[structToHash(t)] = tokenId;
            return t;
        }
        return generate(tokenId, random(seed));
    }

    /**
     * uses A.J. Walker's Alias algorithm for O(1) rarity table lookup
     * ensuring O(1) instead of O(n) reduces mint cost by more than 50%
     * probability & alias tables are generated off-chain beforehand
     * @param seed portion of the 256 bit seed to remove trait correlation
     * @param traitType the trait type to select a trait for
     * @return the ID of the randomly selected trait
     */
    function selectTrait(uint16 seed, uint8 traitType)
        internal
        view
        returns (uint8)
    {
        return traits.selectTrait(seed, traitType);
    }

    function upgradeGenerate(uint256 tokenId, uint256 seed)
        internal
        returns (RichCity memory t)
    {
        t = upgradeTraits(tokenId, seed);
        if (existingCombinations[structToHash(t)] == 0) {
            tokenTraits[tokenId] = t;
            existingCombinations[structToHash(t)] = tokenId;
            return t;
        }
        return upgradeGenerate(tokenId, random(seed));
    }

    function upgradeTraits(uint256 tokenId, uint256 seed)
        internal
        returns (RichCity memory)
    {
        RichCity storage t = tokenTraits[tokenId];
        t.jewelry = 0;
        t.tatto = 0;
        t.body = selectTrait(uint16(seed & 0xFFFF), 25);
        seed >>= 16;
        t.clothes = selectTrait(uint16(seed & 0xFFFF), 26);
        seed >>= 16;
        t.belt = selectTrait(uint16(seed & 0xFFFF), 29);
        seed >>= 16;
        t.hair = selectTrait(uint16(seed & 0xFFFF), 28);
        seed >>= 16;
        t.sunglass = selectTrait(uint16(seed & 0xFFFF), 30);
        return t;
    }

    function selectTraits(uint256 seed)
        internal
        view
        returns (RichCity memory t)
    {
        t.gen = getGen(minted);
        t.isGangster = (seed & 0xFFFF) % 10 != 0;
        uint8 shift = t.isGangster ? 0 : 10; // 0 10 20
        seed >>= 16;
        if (t.isGangster) {
            t.level = selectTrait(uint16(seed & 0xFFFF), 7 + shift);
            seed >>= 16;
            t.body = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t.clothes = selectTrait(uint16(seed & 0xFFFF), 6 + shift);
            seed >>= 16;
            t.hair = selectTrait(uint16(seed & 0xFFFF), 8 + shift);
            seed >>= 16;
            t.jewelry = selectTrait(uint16(seed & 0xFFFF), 9 + shift);
            seed >>= 16;
            t.tatto = selectTrait(uint16(seed & 0xFFFF), 10 + shift);
        } else {
            t.level = selectTrait(uint16(seed & 0xFFFF), 7 + shift);
            seed >>= 16;
            t.body = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t.clothes = selectTrait(uint16(seed & 0xFFFF), 6 + shift);
            seed >>= 16;
            t.mask = selectTrait(uint16(seed & 0xFFFF), 8 + shift);
            seed >>= 16;
            t.weapon = selectTrait(uint16(seed & 0xFFFF), 9 + shift);
            seed >>= 16;
            t.hat = selectTrait(uint16(seed & 0xFFFF), 10 + shift);
        }
    }

    function swapAndAddLiquidity(uint256 totalCost, IERC20 token) internal {
        ISwapV2Pair lpToken = ISwapV2Pair(
            factory.getPair(address(RICH), address(USDC))
        );
        ISwapV2Pair lpToken2 = ISwapV2Pair(
            factory.getPair(address(RICH), address(CASH))
        );
        RICH.approve(address(router), type(uint256).max);
        USDC.approve(address(router), type(uint256).max);
        CASH.approve(address(router), type(uint256).max);
        if (token == USDC) {
            (uint256 reserve0, uint256 reserve1, ) = ISwapV2Pair(lpToken)
                .getReserves();
            uint256 price = address(USDC) == ISwapV2Pair(lpToken).token0()
                ? reserve0.mul(1e7).div(reserve1)
                : reserve1.mul(1e7).div(reserve0);

            USDC.safeTransferFrom(
                msg.sender,
                address(this),
                totalCost.mul(price).mul(101)
            );

            address[] memory path = new address[](2);
            (path[0], path[1]) = (address(USDC), address(RICH));
            router.swapExactTokensForTokens(
                USDC.balanceOf(address(this)),
                0,
                path,
                address(this),
                block.timestamp
            );
        } else {
            RICH.safeTransferFrom(
                msg.sender,
                address(this),
                totalCost.mul(10**RICH.decimals())
            );
        }
        calAndSwap(lpToken2, address(RICH), address(CASH));

        (, , uint256 moreLPAmount) = router.addLiquidity(
            address(CASH),
            address(RICH),
            CASH.balanceOf(address(this)),
            RICH.balanceOf(address(this)).sub(rewardAmount),
            0,
            0,
            address(this),
            block.timestamp
        );
    }

    /// Compute amount and swap between borrowToken and tokenRelative.
    function calAndSwap(
        ISwapV2Pair lpToken,
        address tokenA,
        address tokenB
    ) internal {
        (uint256 token0Reserve, uint256 token1Reserve, ) = lpToken
            .getReserves();
        (uint256 debtReserve, uint256 relativeReserve) = address(RICH) ==
            lpToken.token0()
            ? (token0Reserve, token1Reserve)
            : (token1Reserve, token0Reserve);
        (uint256 swapAmt, bool isReversed) = optimalDeposit(
            RICH.balanceOf(address(this)).sub(rewardAmount),
            CASH.balanceOf(address(this)),
            debtReserve,
            relativeReserve
        );

        if (swapAmt > 0) {
            address[] memory path = new address[](2);
            (path[0], path[1]) = isReversed
                ? (tokenB, tokenA)
                : (tokenA, tokenB);
            router.swapExactTokensForTokens(
                swapAmt,
                0,
                path,
                address(this),
                block.timestamp
            );
        }
    }

    function optimalDeposit(
        uint256 amtA,
        uint256 amtB,
        uint256 resA,
        uint256 resB
    ) internal pure returns (uint256 swapAmt, bool isReversed) {
        if (amtA.mul(resB) >= amtB.mul(resA)) {
            swapAmt = _optimalDepositA(amtA, amtB, resA, resB);
            isReversed = false;
        } else {
            swapAmt = _optimalDepositA(amtB, amtA, resB, resA);
            isReversed = true;
        }
    }

    function _optimalDepositA(
        uint256 amtA,
        uint256 amtB,
        uint256 resA,
        uint256 resB
    ) internal pure returns (uint256) {
        require(amtA.mul(resB) >= amtB.mul(resA), "Reversed");

        uint256 a = 997;
        uint256 b = uint256(1997).mul(resA);
        uint256 _c = (amtA.mul(resB)).sub(amtB.mul(resA));
        uint256 c = _c.mul(1000).div(amtB.add(resB)).mul(resA);

        uint256 d = a.mul(c).mul(4);
        uint256 e = Math.sqrt(b.mul(b).add(d));

        uint256 numerator = e.sub(b);
        uint256 denominator = a.mul(2);

        return numerator.div(denominator);
    }

    /**
     * converts a struct to a 256 bit hash to check for uniqueness
     * @param s the struct to pack into a hash
     * @return the 256 bit hash of the struct
     */
    function structToHash(RichCity memory s) internal pure returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        s.isGangster,
                        s.body,
                        s.clothes,
                        s.hair,
                        s.jewelry,
                        s.tatto,
                        s.belt,
                        s.sunglass,
                        s.hat,
                        s.mask,
                        s.weapon
                    )
                )
            );
    }

    /**
     * generates a pseudorandom number
     * @param seed a value ensure different outcomes for different sources in the same block
     * @return a pseudorandom value
     */
    function random(uint256 seed) internal view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        tx.origin,
                        blockhash(block.number - 1),
                        block.timestamp,
                        seed
                    )
                )
            ) ^ randomSource.seed();
    }

    /***READ */

    function upgradeCost(uint256 tokenId) public view returns (uint256 cost) {
        RichCity memory t = tokenTraits[tokenId];
        if (t.isGangster) {
            cost = 100 * 3**(t.level - 1);
        } else {
            cost = t.level == 1 ? 900 : 2700;
        }
    }

    function mintCost(uint256 tokenId) public view returns (uint256) {
        if (tokenId > 1000) {
            return 123;
        } else {
            return PUBLIC_PRICE;
        }
    }

    function getTokenTraits(uint256 tokenId)
        external
        view
        returns (RichCity memory)
    {
        return tokenTraits[tokenId];
    }

    function getGen(uint256 tokenId) public view returns (uint8) {
        if (tokenId <= 1000) return 0;
        return uint8((tokenId.sub(1001)).div(500).add(1));
    }

    function getMintAccounts() public view returns (uint256) {
        return mintAccounts.length;
    }

    /***ADMIN */
    function setRandomSource(address _randomSource) external onlyOwner {
        randomSource = IRandomSource(_randomSource);
    }

    function withdraw(IERC20 _token) external onlyOwner {
        IERC20(_token).transfer(
            msg.sender,
            IERC20(_token).balanceOf(address(this))
        );
    }

    function changePrice(
        uint256 _PUBLIC_PRICE,
        uint256 _ONE_WHITE_PRICE,
        uint256 _TWO_WHITE_PRICE
    ) public onlyOwner {
        PUBLIC_PRICE = _PUBLIC_PRICE;
        ONE_WHITE_PRICE = _ONE_WHITE_PRICE;
        TWO_WHITE_PRICE = _TWO_WHITE_PRICE;
    }

    function changeSaleTime(uint256 _WHITE_SALE_TIME, uint256 _PUBLIC_SALE_TIME)
        public
        onlyOwner
    {
        WHITE_SALE_TIME = _WHITE_SALE_TIME;
        PUBLIC_SALE_TIME = _PUBLIC_SALE_TIME;
    }

    function setTraits(ITraits addr) public onlyOwner {
        traits = addr;
    }

    function setBank(address _bank) public onlyOwner {
        bank = _bank;
    }

    function whiteListBuyers(
        address[] memory _buyersOne,
        address[] memory _buyersTwo
    ) external onlyOwner {
        for (uint256 i; i < _buyersOne.length; i++) {
            whiteListedOne[_buyersOne[i]] = true;
        }
        for (uint256 i; i < _buyersTwo.length; i++) {
            whiteListedTwo[_buyersTwo[i]] = true;
        }
    }

    /***RENDER */

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(_exists(tokenId), "ERC721Metadata: nonexistent token");
        return traits.tokenURI(tokenId);
    }
}
